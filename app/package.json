{
  "name": "@lyra/app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn workspace @lyrafinance/lyra-js build && concurrently \"yarn workspace @lyrafinance/lyra-js watch\" \"craco start\"",
    "build": "yarn workspace @lyrafinance/lyra-js build && craco build",
    "serve": "serve -s build",
    "analyze": "REACT_APP_INTERACTIVE_ANALYZE=1 yarn build",
    "format": "yarn prettier --write '**/*.{ts,tsx}'",
    "lint": "eslint '**/*.{ts,tsx}'",
    "tsc": "tsc",
    "clean": "rimraf node_modules build"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@gnosis.pm/safe-apps-wagmi": "^1.0.3",
    "@lyra/ui": "^1.0.0",
    "@lyrafinance/lyra-js": "^0.0.21",
    "@rainbow-me/rainbowkit": "0.6.2",
    "@socket.tech/plugin": "^1.0.0",
    "copy-to-clipboard": "^3.3.1",
    "nullthrows": "^1.1.1",
    "posthog-js": "^1.28.0",
    "react": "18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-jazzicon": "^1.0.4",
    "react-router-dom": "^6.4.4",
    "swr": "^1.3.0",
    "wagmi": "0.6.4"
  },
  "peerDependencies": {
    "ethers": "5.6.0",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/node": "17.0.40",
    "@types/react": "18.0.1",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@uniswap/token-lists": "^1.0.0-beta.25",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.7.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "serve": "^14.1.2",
    "typescript": "^4.7.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
